#!/bin/bash
usgs_config=`find_config met.config`
if [ "$usgs_config" = "" ]; then
  usgs_config="$META_MODEL_ROOT/models/om/usgs/usgs.config"
fi
echo "Loading usgs_config from $usgs_config"
. $usgs_config

mscr="$META_MODEL_ROOT/scripts/usgs/usgs_to_om_flows.R"
echo "Rscript $mscr $sta_id $usgs_out_file \"$START_DATE\" \"$END_DATE\" $da"
Rscript $mscr $sta_id out_file.temp "$START_DATE" "$END_DATE" $da
install -D "${tempdir}/${outfile}" $usgs_out_file